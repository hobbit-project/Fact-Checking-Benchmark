@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix hobbit: <http://w3id.org/hobbit/vocab#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix mysets1: <http://w3id.org/mysets1/vocab#> .
@prefix mysets: <http://w3id.org/mysets1/hobbit/vocab#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://project-hobbit.eu/factcheck-benchmark/benchmark> a hobbit:Benchmark;
    rdfs:label "FactCheck Benchmark"@en;
    rdfs:comment    "FactCheck Algorithm benchmark"@en;
    hobbit:imageName    "git.project-hobbit.eu:4567/oshando/factcheck-benchmark/benchmark-controller";
    hobbit:version "v1.0"@en;
    hobbit:measuresKPI <http://project-hobbit.eu/factcheck-benchmark/precision>;
    hobbit:measuresKPI <http://project-hobbit.eu/factcheck-benchmark/recall>;
    hobbit:measuresKPI <http://project-hobbit.eu/factcheck-benchmark/accuracy>;
    hobbit:measuresKPI <http://project-hobbit.eu/factcheck-benchmark/fmeasure>;
    hobbit:measuresKPI <http://project-hobbit.eu/factcheck-benchmark/runTime>;
    hobbit:measuresKPI <http://project-hobbit.eu/factcheck-benchmark/aucRoc>;

    hobbit:hasParameter <http://project-hobbit.eu/factcheck-benchmark/dataPointCount>;
    hobbit:hasParameter <http://project-hobbit.eu/factcheck-benchmark/benchmarkMode>;
    hobbit:hasParameter <http://project-hobbit.eu/factcheck-benchmark/probabilityThreshold>;
    hobbit:hasParameter <http://project-hobbit.eu/factcheck-benchmark/windowSize>;
    hobbit:hasParameter <http://project-hobbit.eu/factcheck-benchmark/transitionsCount>;
    hobbit:hasParameter <http://project-hobbit.eu/factcheck-benchmark/maxClusterIterations>;
    hobbit:hasParameter <http://project-hobbit.eu/factcheck-benchmark/interval>;
    hobbit:hasParameter <http://project-hobbit.eu/factcheck-benchmark/seed>;
    hobbit:hasParameter <http://project-hobbit.eu/factcheck-benchmark/format>;
    hobbit:hasParameter <http://project-hobbit.eu/factcheck-benchmark/machineCount>;
    hobbit:hasParameter <http://project-hobbit.eu/factcheck-benchmark/timeoutMinutes>;

    hobbit:hasAPI <http://project-hobbit.eu/factcheck-benchmark/api>;
    hobbit:hasParameter	mysets:hasDataset .

mysets:hasDataset	a	hobbit:Parameter, hobbit:FeatureParameter, hobbit:ConfigurableParameter;
			rdfs:label	"Dataset name"@en;
			rdfs:comment	"The dataset which should be tested."@en;
			rdfs:domain	hobbit:Benchmark;
			rdfs:range	mysets1:Datasets.

<http://project-hobbit.eu/factcheck-benchmark/benchmark/precision> a hobbit:KPI;
     rdfs:label "Precision"@en;
        rdfs:comment "Precision = truePositive / (truePositive + falsePositive)"@en;
        rdfs:range xsd:double .

<http://project-hobbit.eu/factcheck-benchmark/benchmark/recall> a hobbit:KPI;
     rdfs:label "Recall"@en;
        rdfs:comment "Recall = truePositive  / (truePositive + falseNegative)"@en;
        rdfs:range xsd:double .

<http://project-hobbit.eu/factcheck-benchmark/benchmark/accuracy> a hobbit:KPI;
    rdfs:label "Accuracy"@en;
    rdfs:comment "Accuracy = (truePositive + trueNegative) / Total population"@en;
    rdfs:range xsd:double .

<http://project-hobbit.eu/factcheck-benchmark/benchmark/fmeasure> a hobbit:KPI;
	rdfs:label "F-measure"@en;
	rdfs:comment "F-measure is the harmonic mean of precision and recall."@en;
	rdfs:range xsd:float .

<http://project-hobbit.eu/factcheck-benchmark/benchmark/aucRoc> a hobbit:KPI;
	rdfs:label "ROC curve"@en;
	rdfs:comment "True positive rate is plotted in function of the false positive rate"@en;
	rdfs:range xsd:float .

<http://project-hobbit.eu/resources/factcheck-benchmark/benchmarkMode> a hobbit:ConfigurableParameter, hobbit:FeatureParameter;
	rdfs:label "Benchmark mode. 'static' or 'dynamic:x:y' only"@en;
    rdfs:range xsd:string;
    hobbit:defaultValue "static"^^xsd:string .

<http://project-hobbit.eu/resources/factcheck-benchmark/dataPointCount> a hobbit:ConfigurableParameter, hobbit:FeatureParameter;
	rdfs:label "Amount of messages"@en;
	rdfs:comment "Amount of messages"@en;
	rdfs:range xsd:unsignedInt;
    hobbit:defaultValue "63"^^xsd:unsignedInt .

<http://project-hobbit.eu/resources/factcheck-benchmark/probabilityThreshold> a hobbit:ConfigurableParameter, hobbit:FeatureParameter, hobbit:ForwardedParameter;
    rdfs:label "Probability threshold"@en;
    rdfs:comment "Probability threshold"@en;
    rdfs:range xsd:double;
    hobbit:defaultValue "0.005"^^xsd:double .

<http://project-hobbit.eu/resources/factcheck-benchmark/windowSize> a hobbit:ConfigurableParameter, hobbit:FeatureParameter, hobbit:ForwardedParameter;
    rdfs:label "Window size"@en;
    rdfs:comment "Window size"@en;
    rdfs:range xsd:unsignedInt;
    hobbit:defaultValue "10"^^xsd:unsignedInt .

<http://project-hobbit.eu/resources/factcheck-benchmark/transitionsCount> a hobbit:ConfigurableParameter, hobbit:FeatureParameter, hobbit:ForwardedParameter;
    rdfs:label "Transitions count"@en;
    rdfs:comment "Transitions count"@en;
    rdfs:range xsd:unsignedInt;
    hobbit:defaultValue "5"^^xsd:unsignedInt .

<http://project-hobbit.eu/resources/factcheck-benchmark/maxClusterIterations> a hobbit:ConfigurableParameter, hobbit:FeatureParameter, hobbit:ForwardedParameter;
    rdfs:label "Max. clustering iterations"@en;
    rdfs:comment "Max. clustering iterations"@en;
    rdfs:range xsd:unsignedInt;
    hobbit:defaultValue "50"^^xsd:unsignedInt .

<http://project-hobbit.eu/resources/factcheck-benchmark/interval> a hobbit:ConfigurableParameter, hobbit:FeatureParameter;
    rdfs:label "Interval between measurements (nanos)"@en;
    rdfs:comment "Interval between measurements (nanos)"@en;
    rdfs:range xsd:unsignedInt;
    hobbit:defaultValue "10"^^xsd:unsignedInt .

<http://project-hobbit.eu/resources/factcheck-benchmark/seed> a hobbit:ConfigurableParameter, hobbit:FeatureParameter;
    rdfs:label "seed"@en;
    rdfs:comment "seed"@en;
    rdfs:range xsd:unsignedInt;
    hobbit:defaultValue "123"^^xsd:unsignedInt .

<http://project-hobbit.eu/resources/factcheck-benchmark/format> a hobbit:ConfigurableParameter, hobbit:FeatureParameter;
    rdfs:label "Output format.0-RDF, otherwise CSV"@en;
    rdfs:comment "Output format.0-RDF, otherwise CSV"@en;
    rdfs:range xsd:unsignedInt;
    hobbit:defaultValue "0"^^xsd:unsignedInt .

<http://project-hobbit.eu/resources/factcheck-benchmark/machineCount> a hobbit:ConfigurableParameter, hobbit:FeatureParameter;
    rdfs:label "Machine count. Uses 1000 machines metadata"@en;
    rdfs:label "Machine count. Uses 1000 machines metadata"@en;
    rdfs:range xsd:unsignedInt;
    hobbit:defaultValue "1"^^xsd:unsignedInt .

<http://project-hobbit.eu/resources/factcheck-benchmark/timeoutMinutes> a hobbit:ConfigurableParameter, hobbit:FeatureParameter;
    rdfs:label "Timeout (min.) -1 meaning no timeout"@en;
    rdfs:comment "Timeout (min.) -1 meaning no timeout"@en;
    rdfs:range xsd:int;
    hobbit:defaultValue "-1"^^xsd:int .

mysets1:Datasets a rdfs:Class, owl:Class .

mysets1:CorrectData a mysets1:Datasets;
		rdfs:label "Correct Data"@en;
		rdfs:comment "using the Correct Dataset"@en .

mysets1:WrongData a mysets1:Datasets;
		rdfs:label "Wrong Data"@en;
		rdfs:comment "using the Wrong Dataset"@en .

mysets1:TrainDataSet a mysets1:Datasets;
		rdfs:label "Train Data Set"@en;
		rdfs:comment "using the Train Dataset"@en .

mysets1:TestDataSet a mysets1:Datasets;
		rdfs:label "Test Data Set"@en;
		rdfs:comment "using the Test Dataset"@en .
